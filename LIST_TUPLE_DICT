# LIST >>>> ####

list1 = [1, 2, 3, 4, 5, 6, 7]
list2 = ["roshan", "Artificial", "Intelligence", "Certificate"]
print(list1)
print(list2)
print(list1[1])

# TUPLE >>>> ####

tup1 = ('physics', 'chemistry', 1997, 2000);
tup2 = (1, 2, 3, 4, 5, 6, 7 );
print(tup1[0])
print(tup2[2])

# DICT >>>> ####

dict = {1: "ROSHAN", 2: "SRIVASTAVA", 3: "ARTIFICIAL", 4: "INTELLIGENCE"}
del dict[1]
print(dict)

